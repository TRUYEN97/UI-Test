<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:TabViewModel="clr-namespace:UiTest.ModelView.TabItemViewModel"
                    xmlns:TabView="clr-namespace:UiTest.View.TabItemView"
                    xmlns:ListItemViewModel="clr-namespace:UiTest.ModelView.ListBoxItems"
                    xmlns:ListItemView="clr-namespace:UiTest.View.ListBoxItemViews">
    <DataTemplate DataType="{x:Type TabViewModel:TabLogViewModel}">
        <TabView:TabLogView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TabViewModel:TabMessageViewModel}">
        <TabView:TabMessageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ListItemViewModel:ActionToolModelView}">
        <ListItemView:ActionToolView/>
    </DataTemplate>
</ResourceDictionary>