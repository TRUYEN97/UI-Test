<Application x:Class="UiTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UiTest"
             xmlns:ModelViewlocal="clr-namespace:UiTest.ModelView"
             xmlns:Service="clr-namespace:UiTest.Service"
             xmlns:Viewlocal="clr-namespace:UiTest.View"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/RoundedWindowButton.xaml"/>
                <ResourceDictionary Source="/Themes/Colors.xaml"/>
                <ResourceDictionary Source="/Themes/Shadows.xaml"/>
                <ResourceDictionary Source="/Themes/Generic.xaml"/>
                <ResourceDictionary Source="/Resources/DataTemplates.xaml"/>
                <ResourceDictionary Source="/Resources/TabItemDataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
